{
  "name": "bfet",
  "version": "2.0.0",
  "description": "A thin minimal wrapped around http|https NodeJS module for NodeJS and browser.",
  "main": "lib/bfet.js",
  "scripts": {
    "prepublish": "npm run build",
    "build": "./node_modules/.bin/rimraf dist/* && ./node_modules/.bin/rimraf lib/* && ./node_modules/.bin/babel src -d lib && gulp build",
    "test": "./node_modules/.bin/jasmine && gulp unittest",
    "docs": "gulp doc",
    "start": "gulp buildDev && npm run http-and-watch",
    "http-and-watch": "npm run build && ./node_modules/.bin/lite-server & gulp watch",
    "clean": "./node_modules/.bin/rimraf dist/* && ./node_modules/.bin/rimraf lib/* && ./node_modules/.bin/rimraf docs/*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/haxpor/bfet.git"
  },
  "author": "Wasin Thonkaew",
  "license": "CC-BY-SA",
  "bugs": {
    "url": "https://github.com/haxpor/bfet/issues"
  },
  "keywords": [
    "bfet",
    "http",
    "https",
    "get",
    "post",
    "network",
    "request"
  ],
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-preset-es2015": "^6.16.0",
    "browserify": "^13.1.0",
    "browserify-istanbul": "^0.2.1",
    "bundle-collapser": "^1.2.1",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-concat": "^2.6.0",
    "gulp-file": "^0.3.0",
    "gulp-html-validator": "0.0.5",
    "gulp-insert": "^0.5.0",
    "gulp-jsdoc3": "^1.0.0",
    "gulp-karma": "0.0.5",
    "gulp-replace": "^0.5.4",
    "gulp-streamify": "^1.0.2",
    "gulp-uglify": "^2.0.0",
    "http-browserify": "^1.7.0",
    "jasmine": "^2.5.3",
    "jasmine-core": "^2.5.2",
    "jsdoc": "^3.4.1",
    "karma": "^0.13.22",
    "karma-browserify": "^5.1.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-firefox-launcher": "^1.0.0",
    "karma-html-reporter": "^0.2.7",
    "karma-jasmine": "^1.0.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-safari-launcher": "^1.0.0",
    "karma-spec-reporter": "0.0.26",
    "lite-server": "^2.2.2",
    "remove": "^0.1.5",
    "rimraf": "^2.5.4",
    "source-map": "^0.5.6",
    "uglify-js": "^2.7.3",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.7.0",
    "yargs": "^5.0.0"
  },
  "homepage": "https://github.com/haxpor/bfet#readme"
}
